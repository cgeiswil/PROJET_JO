<script>
      // Get the timeline and navbar elements
var timeline = document.getElementById("timeline");
var navbar = document.getElementById("navbar");

// Update the timeline position when the navbar value changes
navbar.addEventListener("input", function() {
  timeline.scrollLeft = navbar.value / 100 * (timeline.scrollWidth - timeline.clientWidth);
});

    </script>
    
    <input type="range" class="mx-auto d-block" min="0" max="100" value="0" id="navbar">
    
